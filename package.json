{
  "name": "@openxapi/binance",
  "version": "0.2.0",
  "description": "JavaScript/TypeScript client for Binance Spot API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "npx jest --maxWorkers=4 --bail",
    "test:watch": "npx jest --watch",
    "format": "npx prettier --ignore-path .prettierignore --write .",
    "lint": "npx eslint '**/*.ts' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openxapi/binance-js.git"
  },
  "keywords": [
    "binance",
    "api",
    "client",
    "cryptocurrency",
    "trading",
    "exchange"
  ],
  "author": "OpenXAPI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openxapi/binance-js/issues"
  },
  "homepage": "https://github.com/openxapi/binance-js#readme",
  "dependencies": {
    "axios": "^1.7.4",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.3.3",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.24.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
